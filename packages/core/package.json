{
  "name": "@routo/core",
  "version": "0.2.0",
  "description": "Simple router for Redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/exeto/routo.git",
    "directory": "packages/core"
  },
  "main": "lib/index.js",
  "author": "Timofey Dergachev <t@exeto.me> (https://exeto.me/en)",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && babel src -d lib --ignore **/__tests__,**/*.test.js",
    "codecov": "codecov --token=$CODECOV_TOKEN -F core",
    "size": "yarn build && size-limit",
    "test": "jest --coverage && yarn size",
    "prepublishOnly": "yarn test"
  },
  "files": [
    "lib",
    "src",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "keywords": [
    "history",
    "react",
    "redux",
    "route",
    "router",
    "routing"
  ],
  "dependencies": {
    "history": "^4.7.2",
    "path-to-regexp": "^5.0.0",
    "qs": "^6.7.0",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@size-limit/preset-small-lib": "^2.0.2",
    "babel-plugin-import": "^1.11.0",
    "codecov": "^3.3.0",
    "jest": "^24.7.1"
  }
}
